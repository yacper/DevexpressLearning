<Application x:Class="VisualStudioDockingCustomSerialization.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Common.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/LayoutItems.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/BarItems.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>